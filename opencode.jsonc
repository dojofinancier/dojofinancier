
{
    "$schema": "https://opencode.ai/config.json",
    "mcp": {
        "supabase": {
            "type": "remote",
            "url": "https://mcp.supabase.com/mcp?project_ref=akmgrylqxxjzdcdulvps",
            "enabled": true,
            "oauth": {}
        },
        "chrome-devtools": {
            "type": "local",
            "command": [
                "npx",
                "-y",
                "chrome-devtools-mcp@latest",
                "--browser-url=http://127.0.0.1:9222"
            ],
            "environment": {
                "SystemRoot": "C:\\Windows",
                "PROGRAMFILES": "C:\\Program Files"
            },
            "timeout": 20000
        }
    }
}
